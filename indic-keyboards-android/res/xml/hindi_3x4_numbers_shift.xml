<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0967" android:keyIcon="@drawable/hindi_3x4_num_1_hindi" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x0968" android:keyIcon="@drawable/hindi_3x4_num_2_hindi" android:keyWidth="25%p" />
    <Key android:codes="0x0969" android:keyIcon="@drawable/hindi_3x4_num_3_hindi" android:keyWidth="25%p" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="25%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row>
    <Key android:codes="0x096a" android:keyIcon="@drawable/hindi_3x4_num_4_hindi" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x096b" android:keyIcon="@drawable/hindi_3x4_num_5_hindi" android:keyWidth="25%p" />
    <Key android:codes="0x096c" android:keyIcon="@drawable/hindi_3x4_num_6_hindi" android:keyWidth="25%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="25%p"
      android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="0x096d" android:keyIcon="@drawable/hindi_3x4_num_7_hindi" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x096e" android:keyIcon="@drawable/hindi_3x4_num_8_hindi" android:keyWidth="25%p" />
    <Key android:codes="0x096f" android:keyIcon="@drawable/hindi_3x4_num_9_hindi" android:keyWidth="25%p" />
    <Key android:codes="0xF102" android:keyIcon="@drawable/n_to_a" android:keyWidth="25%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="25%p"
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0x0966" android:keyIcon="@drawable/hindi_3x4_num_0_hindi" android:keyWidth="25%p" />
    <Key android:codes="0xF103" android:keyLabel=". , @" android:keyWidth="25%p" />
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="13%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>