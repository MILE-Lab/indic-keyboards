<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="43,45,215,247" android:keyLabel="+ - × ÷" android:keyWidth="25%p" android:keyEdgeFlags="left" />
    <Key android:codes="61,126,177,37" android:keyLabel="= ~ ± %" android:keyWidth="25%p" />
    <Key android:codes="63,47,92,124" android:keyLabel="\? / \\ |" android:keyWidth="25%p" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="25%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row>
    <Key android:codes="35,38,36,94" android:keyLabel="\# &amp; $ ^" android:keyWidth="25%p" android:keyEdgeFlags="left" />
    <Key android:codes="40,91,123,60" android:keyLabel="( [ { &lt;" android:keyWidth="25%p" />
    <Key android:codes="41,93,125,62" android:keyLabel=") ] } &gt;" android:keyWidth="25%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="25%p"
      android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="59,58,33" android:keyLabel="; : !" android:keyWidth="25%p" android:keyEdgeFlags="left" />
    <Key android:codes="39,34,42" android:keyLabel="\' &quot; *" android:keyWidth="25%p" />
    <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="25%p" />
    <Key android:codes="0xF004" android:keyIcon="@drawable/n_to_a" android:keyWidth="25%p" android:keyEdgeFlags="right"
      android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="64,95,176,169" android:keyLabel="\@ _ ° ©" android:keyWidth="25%p" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="25%p" />
    <Key android:codes="0xF003" android:keyLabel="123" android:keyWidth="25%p" />
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="13%p" />
  </Row>
</Keyboard>