<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0c67" android:keyIcon="@drawable/telugu_inscript_num_1_telugu" android:keyEdgeFlags="left" />
    <Key android:codes="0x0c68" android:keyIcon="@drawable/telugu_inscript_num_2_telugu" />
    <Key android:codes="0x0c69" android:keyIcon="@drawable/telugu_inscript_num_3_telugu" />
    <Key android:codes="0x0c6a" android:keyIcon="@drawable/telugu_inscript_num_4_telugu" />
    <Key android:codes="0x0c6b" android:keyIcon="@drawable/telugu_inscript_num_5_telugu" />
    <Key android:codes="0x0c6c" android:keyIcon="@drawable/telugu_inscript_num_6_telugu" />
    <Key android:codes="0x0c6d" android:keyIcon="@drawable/telugu_inscript_num_7_telugu" />
    <Key android:codes="0x0c6e" android:keyIcon="@drawable/telugu_inscript_num_8_telugu" />
    <Key android:codes="0x0c6f" android:keyIcon="@drawable/telugu_inscript_num_9_telugu" />
    <Key android:codes="0x0c66" android:keyIcon="@drawable/telugu_inscript_num_0_telugu" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="95" android:keyIcon="@drawable/sym_underscore" android:keyEdgeFlags="left" />
    <Key android:codes="177" android:keyIcon="@drawable/sym_plus_minus" />
    <Key android:codes="176" android:keyIcon="@drawable/sym_degree" />
    <Key android:codes="247" android:keyIcon="@drawable/sym_division" />
    <Key android:codes="94" android:keyIcon="@drawable/sym_circumflex_accent" />
    <Key android:codes="215" android:keyIcon="@drawable/sym_multiplication" />
    <Key android:codes="43" android:keyIcon="@drawable/sym_plus" />
    <Key android:codes="126" android:keyIcon="@drawable/sym_tilde" />
    <Key android:codes="91" android:keyIcon="@drawable/sym_left_square_bracket" />
    <Key android:codes="93" android:keyIcon="@drawable/sym_right_square_bracket" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="60" android:keyIcon="@drawable/sym_less_than" android:keyEdgeFlags="left" />
    <Key android:codes="62" android:keyIcon="@drawable/sym_greater_than" />
    <Key android:codes="123" android:keyIcon="@drawable/sym_left_curly_bracket" />
    <Key android:codes="125" android:keyIcon="@drawable/sym_right_curly_bracket" />
    <Key android:codes="124" android:keyIcon="@drawable/sym_vertical_bar" />
    <Key android:codes="92" android:keyIcon="@drawable/sym_backward_slash" />
    <Key android:codes="0xF010" android:keyIcon="@drawable/telugu_inscript_ksha" />
    <Key android:codes="0x200D" android:keyIcon="@drawable/sym_zwj" />
    <Key android:codes="169" android:keyIcon="@drawable/sym_copyright" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyEdgeFlags="right" 
      android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="12%p"
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="12%p" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="24%p"
      android:isRepeatable="true" />
    <!-- Key android:codes="0xF013" android:keyIcon="@drawable/telugu_inscript_ottu_r" android:keyWidth="10%p" / -->
    <!-- Key android:codes="0xF015" android:keyIcon="@drawable/telugu_inscript_shra" android:keyWidth="10%p" / -->
    <!-- Key android:codes="0xF014" android:keyIcon="@drawable/telugu_inscript_tra" android:keyWidth="12%p" / -->
    <Key android:codes="44,46" android:keyLabel=", ." android:keyWidth="12%p" />
    <Key android:codes="0xF205" android:keyIcon="@drawable/n_to_a" android:keyWidth="14%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="14%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>