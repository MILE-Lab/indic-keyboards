    <Key android:codes="0x0ba8" android:keyIcon="@drawable/tamil_99_na" android:keyWidth="9.1%p" />
    <Key android:codes="0x0baf" android:keyIcon="@drawable/tamil_99_ya" android:keyWidth="9%p" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0x0bf3" android:keyIcon="@drawable/tamil_99_tamil_day_sign" />
    <Key android:codes="0x0bf4" android:keyIcon="@drawable/tamil_99_tamil_month_sign" />
    <Key android:codes="0x0bf5" android:keyIcon="@drawable/tamil_99_tamil_year_sign" />
    <Key android:codes="0x0bf6" android:keyIcon="@drawable/tamil_99_tamil_debit_sign" />
    <Key android:codes="0x0bff" android:keyIcon="@drawable/tamil_99_tamil_credit_sign" />
    <Key android:codes="0x0bb2" android:keyIcon="@drawable/tamil_99_la" />
    <Key android:codes="0x0bb0" android:keyIcon="@drawable/tamil_99_ra" />
    <Key android:codes="0x0bb4" android:keyIcon="@drawable/tamil_99_llla" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" android:keyEdgeFlags="left" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="13%p" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="35%p"
      android:isRepeatable="true" />
    <Key android:codes="0xF203" android:keyLabel="\?123" android:keyWidth="15%p" />
    <Key android:codes="44,46" android:keyLabel=", ." android:keyWidth="10%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="15%p"
      android:keyEdgeFlags="right" />
