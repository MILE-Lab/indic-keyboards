<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0c9f" android:keyIcon="@drawable/q" android:keyEdgeFlags="left" />
    <Key android:codes="0x0ca1" android:keyIcon="@drawable/w" />
    <Key android:codes="0x0c8e" android:keyIcon="@drawable/e" />
    <Key android:codes="0x0cb0" android:keyIcon="@drawable/r" />
    <Key android:codes="0x0ca4" android:keyIcon="@drawable/t" />
    <Key android:codes="0x0caf" android:keyIcon="@drawable/y" />
    <Key android:codes="0x0c89" android:keyIcon="@drawable/u" />
    <Key android:codes="0x0c87" android:keyIcon="@drawable/i" />
    <Key android:codes="0x0c92" android:keyIcon="@drawable/o" />
    <Key android:codes="0x0caa" android:keyIcon="@drawable/p" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="0x0c85" android:keyIcon="@drawable/a" android:horizontalGap="5%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x0cb8" android:keyIcon="@drawable/s" />
    <Key android:codes="0x0ca6" android:keyIcon="@drawable/d" />
    <Key android:codes="0x0ccd" android:keyIcon="@drawable/f" />
    <Key android:codes="0x0c97" android:keyIcon="@drawable/g" />
    <Key android:codes="0x0cb9" android:keyIcon="@drawable/h" />
    <Key android:codes="0x0c9c" android:keyIcon="@drawable/j" />
    <Key android:codes="0x0c95" android:keyIcon="@drawable/k" />
    <Key android:codes="0x0cb2" android:keyIcon="@drawable/l" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="15%p"
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0x0c9e" android:keyIcon="@drawable/z" />
    <Key android:codes="0x0cb7" android:keyIcon="@drawable/x" />
    <Key android:codes="0x0c9a" android:keyIcon="@drawable/c" />
    <Key android:codes="0x0cb5" android:keyIcon="@drawable/v" />
    <Key android:codes="0x0cac" android:keyIcon="@drawable/b" />
    <Key android:codes="0x0ca8" android:keyIcon="@drawable/n" />
    <Key android:codes="0x0cae" android:keyIcon="@drawable/m" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="15%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="15%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="8" android:keyIcon="@drawable/settings" android:keyWidth="10%p" />

    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="35%p"
      android:isRepeatable="true" />
    <Key android:codes="-6" android:keyLabel="12#%" android:keyWidth="15%p" />
    <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="10%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="15%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>