<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0be7" android:keyIcon="@drawable/tamil_num_1_tamil" android:keyEdgeFlags="left" />
    <Key android:codes="0x0be8" android:keyIcon="@drawable/tamil_num_2_tamil" />
    <Key android:codes="0x0be9" android:keyIcon="@drawable/tamil_num_3_tamil" />
    <Key android:codes="0x0bea" android:keyIcon="@drawable/tamil_num_4_tamil" />
    <Key android:codes="0x0beb" android:keyIcon="@drawable/tamil_num_5_tamil" />
    <Key android:codes="0x0bec" android:keyIcon="@drawable/tamil_num_6_tamil" />
    <Key android:codes="0x0bed" android:keyIcon="@drawable/tamil_num_7_tamil" />
    <Key android:codes="0x0bee" android:keyIcon="@drawable/tamil_num_8_tamil" />
    <Key android:codes="0x0bef" android:keyIcon="@drawable/tamil_num_9_tamil" />
    <Key android:codes="0x0be6" android:keyIcon="@drawable/tamil_num_0_tamil" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="95" android:keyIcon="@drawable/sym_underscore" android:keyEdgeFlags="left" />
    <Key android:codes="177" android:keyIcon="@drawable/sym_plus_minus" />
    <Key android:codes="176" android:keyIcon="@drawable/sym_degree" />
    <Key android:codes="247" android:keyIcon="@drawable/sym_division" />
    <Key android:codes="94" android:keyIcon="@drawable/sym_circumflex_accent" />
    <Key android:codes="215" android:keyIcon="@drawable/sym_multiplication" />
    <Key android:codes="43" android:keyIcon="@drawable/sym_plus" />
    <Key android:codes="126" android:keyIcon="@drawable/sym_tilde" />
    <Key android:codes="91" android:keyIcon="@drawable/sym_left_square_bracket" />
    <Key android:codes="93" android:keyIcon="@drawable/sym_right_square_bracket" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="60" android:keyIcon="@drawable/sym_less_than" />
    <Key android:codes="62" android:keyIcon="@drawable/sym_greater_than" />
    <Key android:codes="123" android:keyIcon="@drawable/sym_left_curly_bracket" />
    <Key android:codes="125" android:keyIcon="@drawable/sym_right_curly_bracket" />
    <Key android:codes="124" android:keyIcon="@drawable/sym_vertical_bar" />
    <Key android:codes="92" android:keyIcon="@drawable/sym_backward_slash" />
    <Key android:codes="0x200D" android:keyIcon="@drawable/sym_zwj" />
    <Key android:codes="169" android:keyIcon="@drawable/sym_copyright" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" android:keyEdgeFlags="left" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="13%p" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="35%p"
      android:isRepeatable="true" />
    <Key android:codes="44,46" android:keyLabel=", ." android:keyWidth="10%p" />
    <Key android:codes="0xF204" android:keyIcon="@drawable/tamil_inscript_num_to_letters" android:keyWidth="15%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="15%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>