<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0d67" android:keyIcon="@drawable/malayalam_inscript_one_malayalam" android:keyEdgeFlags="left" />
    <Key android:codes="0x0d68" android:keyIcon="@drawable/malayalam_inscript_two_malayalam" />
    <Key android:codes="0x0d69" android:keyIcon="@drawable/malayalam_inscript_three_malayalam" />
    <Key android:codes="0x0d6a" android:keyIcon="@drawable/malayalam_inscript_four_malayalam" />
    <Key android:codes="0x0d6b" android:keyIcon="@drawable/malayalam_inscript_five_malayalam" />
    <Key android:codes="0x0d6c" android:keyIcon="@drawable/malayalam_inscript_six_malayalam" />
    <Key android:codes="0x0d6d" android:keyIcon="@drawable/malayalam_inscript_seven_malayalam" />
    <Key android:codes="0x0d6e" android:keyIcon="@drawable/malayalam_inscript_eight_malayalam" />
    <Key android:codes="0x0d6f" android:keyIcon="@drawable/malayalam_inscript_nine_malayalam" />
    <Key android:codes="0x0d66" android:keyIcon="@drawable/malayalam_inscript_zero_malayalam" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="95" android:keyIcon="@drawable/sym_underscore" android:keyEdgeFlags="left" />
    <Key android:codes="177" android:keyIcon="@drawable/sym_plus_minus" />
    <Key android:codes="176" android:keyIcon="@drawable/sym_degree" />
    <Key android:codes="247" android:keyIcon="@drawable/sym_division" />
    <Key android:codes="94" android:keyIcon="@drawable/sym_circumflex_accent" />
    <Key android:codes="215" android:keyIcon="@drawable/sym_multiplication" />
    <Key android:codes="43" android:keyIcon="@drawable/sym_plus" />
    <Key android:codes="126" android:keyIcon="@drawable/sym_tilde" />
    <Key android:codes="91" android:keyIcon="@drawable/sym_left_square_bracket" />
    <Key android:codes="93" android:keyIcon="@drawable/sym_right_square_bracket" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="60" android:keyIcon="@drawable/sym_less_than" android:keyEdgeFlags="left" />
    <Key android:codes="62" android:keyIcon="@drawable/sym_greater_than" />
    <Key android:codes="123" android:keyIcon="@drawable/sym_left_curly_bracket" />
    <Key android:codes="125" android:keyIcon="@drawable/sym_right_curly_bracket" />
    <Key android:codes="124" android:keyIcon="@drawable/sym_vertical_bar" />
    <Key android:codes="92" android:keyIcon="@drawable/sym_backward_slash" />
    <Key android:codes="61460,61461" android:keyIcon="@drawable/malayalam_inscript_ksha" />
    <Key android:codes="0x200D" android:keyIcon="@drawable/sym_zwj" />
    <!-- Key android:codes="2419,2420,2421,2422,2423" android:keyIcon="@drawable/malayalam_inscript_kashmiri_vowel" / -->
    <Key android:codes="169" android:keyIcon="@drawable/sym_copyright" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyEdgeFlags="right" 
      android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="10%p"
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="10%p" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="12%p" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="28%p"
      android:isRepeatable="true" />
    <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="10%p" />
    <Key android:codes="0x0d22" android:keyIcon="@drawable/malayalam_inscript_ddha" />
    <Key android:codes="0xF208" android:keyIcon="@drawable/malayalam_inscript_n_to_a" android:keyWidth="10%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="10%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>