<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0be7" android:keyIcon="@drawable/tamil_3x4_num_1_tamil" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x0be8" android:keyIcon="@drawable/tamil_3x4_num_2_tamil" android:keyWidth="25%p" />
    <Key android:codes="0x0be9" android:keyIcon="@drawable/tamil_3x4_num_3_tamil" android:keyWidth="25%p" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="25%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row>
    <Key android:codes="0x0bea" android:keyIcon="@drawable/tamil_3x4_num_4_tamil" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x0beb" android:keyIcon="@drawable/tamil_3x4_num_5_tamil" android:keyWidth="25%p" />
    <Key android:codes="0x0bec" android:keyIcon="@drawable/tamil_3x4_num_6_tamil" android:keyWidth="25%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="25%p"
      android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="0x0bed" android:keyIcon="@drawable/tamil_3x4_num_7_tamil" android:keyWidth="25%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="0x0bee" android:keyIcon="@drawable/tamil_3x4_num_8_tamil" android:keyWidth="25%p" />
    <Key android:codes="0x0bef" android:keyIcon="@drawable/tamil_3x4_num_9_tamil" android:keyWidth="25%p" />
    <Key android:codes="0xF201" android:keyIcon="@drawable/tamil_3x4_n_to_a" android:keyWidth="25%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="25%p"
      android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
    <Key android:codes="0x0be6" android:keyIcon="@drawable/tamil_3x4_num_0_tamil" android:keyWidth="25%p" />
    <Key android:codes="0xF202" android:keyLabel=". , @" android:keyWidth="25%p" />
    <Key android:codes="0xF000" android:keyIcon="@drawable/settings" android:keyWidth="12%p" />
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="13%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>