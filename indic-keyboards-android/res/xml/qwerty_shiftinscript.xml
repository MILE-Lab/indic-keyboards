<Keyboard xmlns:android="http://schemas.android.com/apk/res/android" android:keyWidth="10%p"
  android:horizontalGap="0px" android:verticalGap="0px" android:keyHeight="@dimen/key_height">

  <Row>
    <Key android:codes="0x0c94" android:keyIcon="@drawable/inscript_q1" android:keyEdgeFlags="left" />
    <Key android:codes="0x0c90" android:keyIcon="@drawable/inscript_w1" />
    <Key android:codes="0x0c86" android:keyIcon="@drawable/inscript_e1" />
    <Key android:codes="0x0c88" android:keyIcon="@drawable/inscript_r1" />
    <Key android:codes="0x0c8a" android:keyIcon="@drawable/inscript_t1" />
    <Key android:codes="0x0cad" android:keyIcon="@drawable/inscript_y1" />
    <Key android:codes="0x0c99" android:keyIcon="@drawable/inscript_u1" />
    <Key android:codes="0x0c98" android:keyIcon="@drawable/inscript_i1" />
    <Key android:codes="0x0ca7" android:keyIcon="@drawable/inscript_o1" />
    <Key android:codes="0x0c9d" android:keyIcon="@drawable/inscript_p1" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="0x0c93" android:keyIcon="@drawable/inscript_a1" android:keyEdgeFlags="left" />
    <Key android:codes="0x0c8f" android:keyIcon="@drawable/inscript_s1" />
    <Key android:codes="0x0c85" android:keyIcon="@drawable/inscript_d1" />
    <Key android:codes="0x0c87" android:keyIcon="@drawable/inscript_f1" />
    <Key android:codes="0x0c89" android:keyIcon="@drawable/inscript_g1" />
    <Key android:codes="0x0cab" android:keyIcon="@drawable/inscript_h1" />
    <Key android:codes="0x0caf" android:keyIcon="@drawable/inscript_j1" />
    <Key android:codes="0x0c96" android:keyIcon="@drawable/inscript_k1" />
    <Key android:codes="0x0ca5" android:keyIcon="@drawable/inscript_l1" />
    <Key android:codes="0x0c9a" android:keyIcon="@drawable/inscript_cha1" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="0x0c92" android:keyIcon="@drawable/inscript_oo1" android:keyEdgeFlags="left" />
    <Key android:codes="0x0c8e" android:keyIcon="@drawable/inscript_b1" />
    <Key android:codes="0x0c8b" android:keyIcon="@drawable/inscript_z1" />
    <Key android:codes="0x0c83" android:keyIcon="@drawable/inscript_x1" />
    <Key android:codes="0x0ca3" android:keyIcon="@drawable/inscript_c1" />
    <Key android:codes="0x0c9e" android:keyIcon="@drawable/inscript_v1" />
    <Key android:codes="0x0cb7" android:keyIcon="@drawable/inscript_ya1" />
    <Key android:codes="0x0cb3" android:keyIcon="@drawable/inscript_n1" />
    <Key android:codes="0x0cb6" android:keyIcon="@drawable/inscript_m1" />
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="10%p"
      android:keyEdgeFlags="right" android:isRepeatable="true" />
  </Row>

  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" android:keyWidth="12%p"
      android:keyEdgeFlags="left" />
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="10%p"
      android:isModifier="true" android:isSticky="true" />
    <Key android:codes="0x0c9f" android:keyIcon="@drawable/inscript_ta1" />
    <Key android:codes="0x0ca1" android:keyIcon="@drawable/inscript_da1" />
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="18%p"
      android:isRepeatable="true" />
    <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="10%p" />
    <Key android:codes="1" android:keyLabel="123" android:keyWidth="10%p" />
    <Key android:codes="8" android:keyIcon="@drawable/settings" android:keyWidth="10%p" />
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="10%p"
      android:keyEdgeFlags="right" />
  </Row>
</Keyboard>